{
    "name": "Python Docker",
    "image": "cropgen",
    "appPort": 5000,
    "extensions": ["ms-python.python"],
    "settings": {
      "python.pythonPath": "/usr/local/bin/python"
    },
    "workspaceMount": "source=${localWorkspaceFolder},target=/app,type=bind,consistency=delegated",
    "workspaceFolder": "/app"
  }
  